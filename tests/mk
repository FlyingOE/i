set -e
set -x

mkdir -p out

kk=/c/k/ktye.github.io/kweb/k.k

k math.k -e                                      >out/math
k 66 ../apl/apl.k -e                             >out/apl
k token.k -e                                     >out/token
k $kk ../web/kweb/compile.k -e 'x:`<drawtree P'  >out/compile
k $kk ../x/go.k -e 'x:`<go``'                    >out/go
k $kk ../x/wa.k -e 'x:`<wa``'                    >out/wa
k $kk ../x/wb.k -e 'x:`<wb``'                    >out/wb
k $kk ../x/cc.k -e 'x:`<cc``'                    >out/cc
k $kk ../x/js.k -e 'x:`<js``'                    >out/js
k $kk ../x/qb.k -e 'x:`<qb``'                    >out/qb
k $kk ../kos/softfloat.k ../x/cc.k -e 'x:`<cc``' >out/softfloat
k q.k q.t -e                                     >out/q
k k.t -e                                         >out/k

