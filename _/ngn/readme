# e.g. on windows with msys2/gcc

cc=gcc
ngn=/c/k/ngn/k
set -x
set -e
for c in a b c e f h j k o p s v w x; do
	$cc -c $ngn/${c}.c
done
for c in i m main; do
	$cc -I$ngn -c ${c}.c
done
$cc *.o
rm *.o


# wasm
#  sh mk
