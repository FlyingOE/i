set -e
set -x

k=kosmic

c9=/c/local/cross9/bin

$c9/x86_64-pc-linux-gnu-gcc.exe -g -O -o ${k}.com.dbg ${k}.c               \
  -static -fno-pie -no-pie -mno-red-zone -fno-omit-frame-pointer -nostdlib -nostdinc \
  -Wl,--gc-sections -Wl,-z,max-page-size=0x1000 -fuse-ld=bfd                         \
  -Wl,-T,ape.lds -include cosmopolitan.h crt.o ape.o cosmopolitan.a

$c9/x86_64-pc-linux-gnu-objcopy.exe -S -O binary ${k}.com.dbg ${k}.com
